{% extends 'base.html.twig' %}

{% block title %}Edit Location{% endblock %}

{% block body %}
    <h1>Edit Location</h1>

    {{ form_start(form) }}
    {{ form_row(form.city) }}
    {{ form_row(form.country) }}
    <button class="btn btn-primary">Save changes</button>
    {{ form_end(form) }}

    <a href="{{ path('app_location_index') }}">Back to list</a>
{% endblock %}
